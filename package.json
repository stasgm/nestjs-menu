{
  "name": "nestjs-menu",
  "version": "0.0.1",
  "description": "Simple API for menu-project",
  "author": "Stanislau Shliakhtsich",
  "private": true,
  "license": "MIT",
  "engines": {
    "yarn": ">=1.22",
    "node": ">=18.10.0"
  },
  "scripts": {
    "build": "yarn clear-dist && nest build",
    "check": "tsc --noemit",
    "clear-dist": "rimraf ./dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:check": "tsc --noEmit",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --check \"{src,apps,libs,test}/**/*.ts\"",
    "prettier:fix": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "dev:init": "yarn dev:db:start && yarn dev:db-migrate",
    "dev:db-migrate": "yarn dev-env ts-node ./src/modules/core/prisma/prisma-migrate.ts",
    "dev:db-seed": "yarn dev-env yarn seed",
    "dev:db:start": "docker compose -f docker-compose.dev.yml up -d && sleep 1",
    "dev-env": "NODE_ENV=dev",
    "dev:test": "yarn dev-env yarn test jest",
    "dev:test:e2e": "yarn dev-env yarn test:e2e",
    "dev:test:int": "yarn dev-env yarn test:int",
    "test": "yarn dev-env jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci:cov": "jest --ci --reporters=default --reporters=jest-junit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:int": "jest -i --no-cache --config jest-int.json",
    "prod-env:start": "yarn prod-env yarn start:prod",
    "prod-env:init": "yarn prod-env:db-migrate && yarn prod-env:db-seed",
    "prod-env:db-migrate": "yarn prod-env ts-node ./src/modules/core/prisma/prisma-migrate.ts",
    "prod-env:db-seed": "yarn prod-env yarn seed",
    "prod-env": "dotenv",
    "t-env:init": "yarn t-env:db:start && yarn t-env:db-migrate && yarn t-env:db-seed",
    "t-env:test": "yarn t-env jest",
    "t-env:test:cov": "yarn t-env yarn test:cov",
    "t-env:test:ci:cov": "yarn t-env yarn test:ci:cov",
    "t-env:test:int": "yarn t-env yarn test:int",
    "t-env:test:e2e": "yarn t-env yarn test:e2e",
    "t-env:db:start": "docker compose -f docker-compose.test.yml up -d && sleep 1",
    "t-env:db-migrate": "yarn t-env ts-node ./src/modules/core/prisma/prisma-migrate.ts",
    "t-env:db-seed": "yarn t-env yarn seed",
    "t-env:start:dev": "yarn t-env nest start --watch",
    "t-env": "NODE_ENV=test",
    "migrate": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "generate": "npx prisma generate"
  },
  "dependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "4.15.0",
    "@prisma/migrate": "^4.15.0",
    "@swc/core": "^1.3.61",
    "@swc/jest": "^0.2.26",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "dotenv-cli": "^7.2.1",
    "prisma": "^4.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typescript": "^5.0.4",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "jest": "29.5.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "tsconfig-paths": "4.2.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
