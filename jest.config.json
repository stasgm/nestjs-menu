{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "./",
  "modulePaths": [
    "<rootDir>"
  ],
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(ts|js)?$": [
      "@swc/jest"
    ]
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
